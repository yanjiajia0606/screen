\"false\"></component-list>\n      <span slot=\"footer\"\n            class=\"dialog-footer\">\n        <el-button type=\"primary\"\n                   @click=\"handleApiSubmit()\"\n                   size=\"small\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport codeedit from '../../page/group/code';\nimport componentList from '../../page/list/components'\nimport { getObj } from '@/api/components'\nexport default {\n  name: 'vue',\n  inject: [\"main\"],\n  components: {\n    componentList,\n    codeedit\n  },\n  data () {\n    return {\n      api: {\n        box: false,\n        item: {}\n      },\n      code: {\n        type: 'content',\n        box: false,\n        obj: {},\n      }\n    }\n  },\n  methods: {\n    handleApiSubmit () {\n      getObj(this.api.item.id).then(res => {\n        const data = res.data.data;\n        this.main.activeOption[this.code.type] = data.content\n        this.api.box = false;\n        this.api.item = {}\n      })\n    },\n    handleApiChange (val) {\n      this.api.item = val\n    },\n    codeClose (value) {\n      this.main.activeOption[this.code.type] = value;\n    },\n    openCode () {\n      this.code.obj = this.main.activeOption[this.code.type];\n      this.code.box = true;\n    },\n  }\n}\n</script>\n\n<style>\n</style>"]}]}